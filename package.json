    "upload",
    "chunk",
    "chunk-upload",
    "chunked-upload",
    "resume",
    "resumable",
    "resumable-upload",
    "component",
    "reactnative",
    "react-native",
    "react native",
    "android",
    "ios",
    "javascript"
  ],
  "dependencies": {
    "react-native-fs": "^2.16.6",
    "rn-fetch-blob": "^0.12.0"
  },
  "author": "Hossein Zare",
